{
    "name": "medusa-plugin-settings",
    "version": "0.3.3",
    "description": "Medusa Plugin Settings is a plugin made for medusa that will make it easier to add new settings with ui to your admin dashboard.",
    "author": "Raiden",
    "main": "dist/index.js",
    "repository": {
      "type": "git",
      "url": "https://github.com/raideno/medusa-plugin-settings"
    },
    "homepage": "https://medusa-plugin-settings.raideno.xyz",
    "license": "MIT",
    "keywords": [
      "medusa",
      "medusa-plugin",
      "medusa-plugin-settings",
      "medusa-plugin-admin",
      "medusa-plugin-other",
      "dashboard",
      "user-interface",
      "entity-management",
      "CRUD",
      "configuration",
      "customization",
      "data-management",
      "ecommerce",
      "store",
      "database"
    ],
    "scripts": {
      "clean": "cross-env ./node_modules/.bin/rimraf dist",
      "watch": "cross-env tsc --watch",
      "dev": "cross-env npm run build:server && medusa develop",
      "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
      "build:server": "cross-env npm run clean && tsc -p tsconfig.json",
      "build:admin": "cross-env medusa-admin build",
      "prepare": "cross-env NODE_ENV=production npm run build:server && medusa-admin bundle"
    },
    "peerDependencies": {
      "@medusajs/icons": "^1.0.0",
      "@medusajs/medusa": "^1.16.0",
      "@medusajs/ui": "^2.2.0"
    },
    "dependencies": {
      "@medusajs/admin": "^7.1.1",
      "@medusajs/icons": "^1.0.0",
      "@medusajs/medusa": "^1.16.0",
      "@medusajs/ui": "^2.2.0",
      "@tanstack/react-query": "4.22.0",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "dotenv": "16.0.3",
      "express": "^4.17.2",
      "install": "^0.13.0",
      "medusa-interfaces": "^1.3.7",
      "prism-react-renderer": "^2.0.4",
      "react-router-dom": "^6.16.0",
      "swr": "^2.2.4",
      "typeorm": "^0.3.16"
    },
    "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/node": "^17.0.8",
      "babel-preset-medusa-package": "^1.1.13",
      "cross-env": "^7.0.3",
      "eslint": "^6.8.0",
      "rimraf": "^3.0.2",
      "ts-loader": "^9.2.6",
      "typescript": "^4.5.2"
    }
  }
  